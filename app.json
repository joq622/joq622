{
  "name": "JOQUER-ISHLY-GAG",
  "version": "2.4.0",
  "description": "Joquer-Ishly-Gag, the ultimate WhatsApp Bot with AI capabilities",
  "logo": "https://files.catbox.moe/n6guny.jpg",
  "keywords": ["whatsapp", "bot", "md", "joquer-ishly", "iconic-tech"],
  "repository": "https://github.com/joq622/joq622.git",
  "stack": "container",
  "scripts": {
    "start": "node index.js"
  },
  "env": {
    "BOT_NAME": {
      "description": "Enter your bot name here",
      "value": "JOQUER-ISHLY-GAG",
      "required": true
    },
    "OWNER_NUMBER": {
      "description": "Enter your number with country code (e.g., 255xxxxxxx for Tanzania,Start with your country code without +)",
      "value": "255752429665",
      "required": true
    },
    "OWNER_NAME": {
      "description": "Owner name",
      "value": "Joquer",
      "required": true
    },
    "STICKER_AUTHOR_NAME": {
      "description": "Sticker pack author name",
      "value": "JOQUER",
      "required": false
    },
    "HEROKU_API_KEY": {
      "description": "Heroku API key (leave empty if not using Heroku)",
      "value": "",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name (must match exactly)",
      "value": "",
      "required": false
    },
    "TIMEZONE": {
      "description": "Timezone (e.g., Africa/Harare)",
      "value": "Africa/Nairobi",
      "required": false
    },
    "GITHUB_TOKEN": {
      "description": "GitHub token with 'repo' scope (Generate at: https://github.com/settings/tokens)",
      "value": "",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}
